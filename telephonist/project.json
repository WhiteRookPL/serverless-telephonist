{
  "name": "serverless-telephonist",

  "authors": [ "Wojciech Gawronski (afronski)" ],
  "company": "White Rook",
  "language": "en-US",
  "title": "Serverless Telephonist (On-Call Number Rotation)",
  "description": "An serverless .NET Core example built with use of AWS.",

  "version": "1.0.0-*",

  "buildOptions": {
    "compilerName": "fsc",
    "emitEntryPoint": false,
    "compile": {
      "includeFiles": [
        "OperatorsAPI.fs"
      ]
    },
    "debugType": "portable"
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },

    "Amazon.Lambda.Tools" : {
      "type" :"build",
      "version":"1.5.0"
    },

    "Microsoft.FSharp.Core.netcore": {
      "type": "platform",
      "version": "1.0.0-alpha-*"
    },

    "Newtonsoft.Json": "9.0.1",

    "Amazon.Lambda.Core": "1.0.0*",
    "Amazon.Lambda.Serialization.Json": "1.0.0"
  },

  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-*",
      "imports": [
        "dnxcore50"
      ]
    },

    "Amazon.Lambda.Tools": "1.6.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  }
}
